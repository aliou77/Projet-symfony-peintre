<?xml version="1.0" encoding="UTF-8"?>

<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">

    {% for u in urls %}
        <url>
    
            <loc>{{hostname}} {{u.loc}}</loc>
    
            {% if u.lasmod is defined %}
                <lastmod>{{u.lastmod}}</lastmod>
            {% endif %}
    
        </url>
    {% endfor %}

</urlset> 
